{
	"name": "c360",
	"version": "0.0.1",
	"private": true,
	"prisma": {
		"seed": "node seed.js"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@material/typography": "^14.0.0",
		"@smui/button": "^7.0.0-beta.0",
		"@smui/data-table": "^7.0.0-beta.0",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/menu": "^7.0.0-beta.0",
		"@smui/paper": "^7.0.0-beta.0",
		"@smui/tab": "^7.0.0-beta.0",
		"@smui/tab-bar": "^7.0.0-beta.0",
		"@smui/top-app-bar": "^7.0.0-beta.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.8.1",
		"sass": "^1.57.1",
		"smui-theme": "^7.0.0-beta.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "latest",
		"@auth/sveltekit": "latest",
		"@prisma/client": "^4.8.1"
	}
}
